! special
#define FOREGROUND    #c0c0c0
#define BACKGROUND    #121212
#define CURSORCOLOR   #c0c0c0
                      
!define BLACK         
#define COLOR0        #080808
#define COLOR8        #36393f
                      
!define RED           
#define COLOR1        #515978
#define COLOR9        #778099
                      
!define GREEN         
