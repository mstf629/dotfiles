#define COLOR10       #71c4ba  
                      
!define YELLOW        
#define COLOR3        #4dc5ce
#define COLOR11       #5e767f
                      
!define BLUE          
#define COLOR4        #6ea2a5
#define COLOR12       #6abdc1
                      
!define MAGENTA       
#define COLOR5        #a8a8a8
#define COLOR13       #4e4d80
                      
!define CYAN          
#define COLOR6        #558188
#define COLOR14       #4fc8ce
                      
!define WHITE         
#define COLOR7        #c0c0c0
#define COLOR15       #a6a8aa

! special
*.foreground:   FOREGROUND
*.background:   BACKGROUND
*.cursorColor:  CURSORCOLOR

! black
*.color0:       COLOR0
